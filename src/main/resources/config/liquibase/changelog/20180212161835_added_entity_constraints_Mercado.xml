<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Mercado.
    -->
    <changeSet id="20180212161835-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="sucursal_id"
                                 baseTableName="mercado"
                                 constraintName="fk_mercado_sucursal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sucursal"/>

        <addForeignKeyConstraint baseColumnNames="carro_id"
                                 baseTableName="mercado"
                                 constraintName="fk_mercado_carro_id"
                                 referencedColumnNames="id"
                                 referencedTableName="carro"/>

    </changeSet>
</databaseChangeLog>
