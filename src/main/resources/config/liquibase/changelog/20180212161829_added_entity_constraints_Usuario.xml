<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Usuario.
    -->
    <changeSet id="20180212161829-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

        <addForeignKeyConstraint baseColumnNames="comercio_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario_comercio_id"
                                 referencedColumnNames="id"
                                 referencedTableName="comercio"/>

        <addForeignKeyConstraint baseColumnNames="repartidor_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario_repartidor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="repartidor"/>

    </changeSet>
</databaseChangeLog>
